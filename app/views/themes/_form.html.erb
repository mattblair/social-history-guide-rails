<%= simple_form_for(@theme) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :summary %>
    <%= f.input :image_name %>
    <%= f.input :image_credit %>
    <%= f.input :twitter_template %>
    <%= f.input :editorial_notes %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :displayOrder %>
    <%= f.input :keywords %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
